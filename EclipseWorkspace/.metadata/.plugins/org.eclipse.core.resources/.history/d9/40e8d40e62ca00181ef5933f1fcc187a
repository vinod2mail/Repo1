package Dev_06_08_18;

public class prinmultiplication {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num=5;
		int a=3;
		
		  for (int i = num; i >= 1; i--) 
	        {
	            for (int j = 1; j <= i; j++)
	            {
	                System.out.print(j+" ");
	            }
	             
	            System.out.println();
	        }
			System.out.println();
		}

	}


