package Dev_06_08_18;

public class prinmultiplication {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num=5;
		int a=3;
		
		for(int i=num;i>0;i--)
		{
			for(int j=i;j>0;j--)
			{
				System.out.print(" ");
			}
			
			for(int k=i;k<n;k++)
			{
				System.out.print(k*a+" ");
				
			}
			System.out.println();
		}

	}

}
